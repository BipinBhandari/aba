{
    "name": "EduMonk",
    "structure":{
        "definition": [{
            "name": "system",
            "root": "app"
        }, {
            "name": "module",
            "root": "modules"
        }, {
            "name": "type",
            "root": null
        }]
    },
    "commands": [
        {
            "name": "generate",
            "command": "generate <system> <module> <type> <name>",
            "description": "generate",
            "options": [{
                "short": "-s",
                "long":"--withRedux",
                "description":"with redux integration"
            }, {
                "short": "--type <type>",
                "possible_options": ["stateless", "functional", "nd"],
                "description":"component type"
            }],
            "action": "./aba/generators/generate"
        }
    ]
    
}